ALGORITHM insert_sort
VAR
    A : ARRAY_OF INTEGER[];
    i : INTEGER
BEGIN
FOR i FROM 1 TO A-1 STEP 1  DO
    value:= A[i]
    idx:=i-1
    WHILE (idx>=0 && A[idx]>value) DO
    A[idx+1]:=A[idx]
    idx:= idx+1
    a[idx+1]=value
    END_WHILE
END_FOR
END

